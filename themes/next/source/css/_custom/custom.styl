// Custom styles.

kbd {
    margin-left: 5px;
    margin-right: 5px;
    padding-top: 0px;
    padding-bottom: 7px;
    padding-left: 4px;
    padding-right: 8px;
    font-weight: 600;
    color: #fff;
    border-right-style: solid;
    border-bottom-style: solid;
    border-width: 3px;
    border-right-color: rgba(0,0,0,0.5);
    border-bottom-color: #000;
    border-radius: 5px;
    background-color: rgba(0,0,0,0.5);
}